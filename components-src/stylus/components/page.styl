// internal variables
var-background-color--page := #EFEFF4

.page
  reset-font()
  background-color var-background-color--page
  position absolute
  top 0px
  left 0px
  right 0px
  bottom 0px
  overflow hidden

.page ::-webkit-scrollbar
  display none

